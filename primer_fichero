linea1 desde rama main 

lista = [100,89,75,27,3]
def doPasada(lista):
    resultado = []
    if len(lista) == 1:
        resultado = lista
        return resultado
    if lista[0] > lista[1]:
        aux = lista[0]
        lista[0] = lista[1]
        lista[1] = aux
    resultado.append(lista[0])
    resultado = resultado + doPasada(lista[1:])
    return resultado
print(doPasada(lista))
